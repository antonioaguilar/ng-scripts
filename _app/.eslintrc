{
  "plugins": [],
  "env"    : {
    "es6"          : true,
    "browser"      : true,
    "node"         : true,
    "commonjs"     : true,
    "worker"       : true,
    "amd"          : true,
    "mocha"        : true,
    "jasmine"      : true,
    "phantomjs"    : true,
    "protractor"   : true,
    "prototypejs"  : true,
    "serviceworker": true,
    "webextensions": true,
    "jquery"       : true
  },
  "rules"  : {
    "strict"                          : 0,
    "default-case"                    : 1,
    "no-alert"                        : 2,
    "no-caller"                       : 2,
    "no-bitwise"                      : 0,
    "no-console"                      : 1,
    "no-debugger"                     : 2,
    "no-unused-expressions"           : 1,
    "no-eval"                         : 2,
    "no-ex-assign"                    : 1,
    "no-floating-decimal"             : 1,
    "no-implied-eval"                 : 1,
    "no-with"                         : 1,
    "no-fallthrough"                  : 1,
    "no-unreachable"                  : 2,
    "no-underscore-dangle"            : 0,
    "no-inline-comments"              : 2,
    "no-warning-comments"             : 1,
    "no-undef-init"                   : 2,
    "no-octal"                        : 1,
    "no-obj-calls"                    : 1,
    "no-new-wrappers"                 : 1,
    "no-new"                          : 1,
    "no-new-func"                     : 1,
    "no-native-reassign"              : 1,
    "no-plusplus"                     : 0,
    "no-delete-var"                   : 1,
    "no-return-assign"                : 2,
    "no-new-object"                   : 2,
    "no-label-var"                    : 2,
    "no-ternary"                      : 0,
    "no-self-compare"                 : 2,
    "curly"                           : 0,
    "dot-notation"                    : 0,
    "eqeqeq"                          : 2,
    "new-parens"                      : 1,
    "guard-for-in"                    : 0,
    "radix"                           : 0,
    "new-cap"                         : 0,
    "quote-props"                     : 0,
    "semi"                            : 2,
    "use-isnan"                       : 1,
    "quotes"                          : [2, "single"],
    "max-params"                      : [
      0,
      3
    ],
    "max-statements"                  : [
      0,
      10
    ],
    "complexity"                      : [
      0,
      11
    ],
    "wrap-iife"                       : 1,
    "no-multi-str"                    : 1,
    "no-multi-spaces"                 : 1,
    "no-trailing-spaces"              : 0,
    "key-spacing"                     : 0,
    "brace-style"                     : 0,
    "no-unused-vars"                  : 0,
    "no-shadow"                       : 0,
    "no-undef"                        : 0,
    "camelcase"                       : 0,
    "comma-spacing"                   : 1,
    "no-use-before-define"            : 0
  },
  "globals": {
    "cordova"             : true,
    "StatusBar"           : true,
    "loki"                : true,
    "document"            : true,
    "jsforce"             : true,
    "map"                 : true,
    "window"              : true,
    "before"              : true,
    "beforeEach"          : true,
    "inject"              : true,
    "$log"                : true,
    "_"                   : true,
    "after"               : true,
    "angular"             : true,
    "require"             : true,
    "module"              : true,
    "define"              : true,
    "brackets"            : true,
    "jQuery"              : true,
    "$"                   : true,
    "Mustache"            : true,
    "console"             : true,
    "jasmine"             : true,
    "exports"             : true,
    "browser"             : true,
    "it"                  : true,
    "by"                  : true,
    "waits"               : true,
    "expect"              : true,
    "runs"                : true,
    "element"             : true,
    "waitsFor"            : true,
    "xit"                 : true,
    "describe"            : true,
    "xdescribe"           : true,
    "afterEach"           : true,
    "spyOn"               : true,
    "moment"              : true,
    "performance"         : true
  }
}

